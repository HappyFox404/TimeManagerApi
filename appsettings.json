{
  "TokenSetings": {
    "Issuer": "TimeManagerApi",
    "Audience": "TimeManagerClient",
    "Key": "Test123"
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=TimeManager;Username=postgres;Password=12345rvs"
  },
  "NLog": {
    "throwConfigExceptions": true,
    "extensions": [
      { "assembly": "NLog.Extensions.Logging" },
      { "assembly": "NLog.Web.AspNetCore" }
    ],
    "targets": {
      "async": true,
      "LogInConsole": {
        "type": "Console",
        "layout": "${longdate} ${callsite} ${level} ${message}${newline}"
      }
    },
    "rules": [
      {
        "logger": "Microsoft.AspNetCore.*",
        "maxLevel": "Info",
        "final": true
      },
      {
        "logger": "Microsoft.EntityFrameworkCore.*",
        "maxLevel": "Info",
        "final": true
      },
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "LogInConsole"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
